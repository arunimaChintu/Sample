<mvc:View
  controllerName="project4.controller.View1"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m">
  <Page title="Project 4">
    <content>
    <Input value="{ui/Productname}" placeholder="Productname" width="250px" class="sap"></Input>
  <Input value="{ui/ProductDescription}" placeholder="ProductDescription" width="250px"></Input>
      <List
        id="productList"
        headerText="{view>Products}"
        items="{/Categories}">
        <StandardListItem
          title="{Name}"  
           />
      </List>
    </content>
  </Page>
</mvc:View>
