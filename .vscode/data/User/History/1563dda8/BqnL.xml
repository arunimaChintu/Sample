<mvc:View
    controllerName="project2.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">

    <Page id="page" title="{i18n>title}">
        <List
            headerText="Products"
            headerLevel="H2"
            items="{/Products}">
            
            <StandardListItem
                title="{ProductName}"
                description="{UnitPrice}"
                info="{UnitsInStock}" />
        </List>
    </Page>

</mvc:View>
