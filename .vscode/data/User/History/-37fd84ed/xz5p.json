{
  "_version": "1.76.0",
  "sap.app": {
    "id": "project4",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project4.i18n.i18n",
          "supportedLocales": [
            "en"
          ],
          "fallbackLocale": "en"
        }
      },
      "odata": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "useBatch": false
        }
      }
    },
    "rootView": {
      "viewName": "project4.view.App",
      "type": "XML",
      "id": "app",
      "async": true
    }
  }
}
