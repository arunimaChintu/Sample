<mvc:View controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
    <List
		headerText="Products"
		headerLevel="H2"
		items="{/Products}" >
		<StandardListItem
			title="{Name}"
			counter="{Quantity}"/>
	</List>
    </Page>
</mvc:View>