{
    "editor.semanticTokenColorCustomizations": {
        "[Default Dark+]": {
            "rules": {
                "property.mandatory:xml": {
                    "bold": true
                },
                "string.i18n:xml": {
                    "italic": true
                }
            }
        },
        "[Default Light+]": {
            "rules": {
                "property.mandatory:xml": {
                    "bold": true
                },
                "string.i18n:xml": {
                    "italic": true
                }
            }
        },
        "[Default High Contrast]": {
            "rules": {
                "property.mandatory:xml": {
                    "bold": true
                },
                "string.i18n:xml": {
                    "italic": true
                }
            }
        }
    },
    "git.enableSmartCommit": true,
    "workbench.colorTheme": "Visual Studio Dark",
    "git.autofetch": true,
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command"
    ]
}